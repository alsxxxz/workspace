package School;


import java.io.*;
import java.util.*;

public class PermMain{
		static int N=4;
		static int R=3;
		static int C=0; 
		
		static int[] a= {1,2,3,4};
		static int[] b= new int[R];
		static boolean[] v= new boolean[N];

	static void perm(int cnt) { //depth, index 둘 다 가능.
		//재귀호출 id,else로 되어있음.
		//cnt라는 변수는 바뀌지 않고 read only
		if(cnt == R){
			System.out.println(Arrays.toString(b)); C++;
			return;
		}
		for(int i=0; i<N; i++) {
			if(v[i]) continue;
			v[i] = true; //방문처리
				b[cnt] = a[i];
					perm(cnt+1);
				v[i] = false;
									}
				}
								}
	
	
	public static void main(String[] args) throws IOException{
		C=0; 
		perm(0);
		System.out.println(C);
	}
	
}
